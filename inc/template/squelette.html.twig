<body id="body">
	<div id="search-overlay" class="pal">
		<div id="close-search-overlay" class="close-search-overlay pam">
			&times;
		</div>
		<form class="form-search" id="form-search">
			<input type="text" id="search-keywords" name="keywords" placeholder="Search for kittens ..." class="proxima italic pam" />
			<label for="submit-form-search" id="label-submit-search" class="pam">
				<i class="fa fa-search pls"></i>
			</label>
			<input type="submit" id="submit-form-search" />
		</form>
	</div>
	<div id="bloc-body" class="bloc-body transition-medium proxima">
		<div class="menu-barre">
			<div class="menu-barre-body">
				<div class="menu-barre-logo inbl proxima-bold h4-like pam transition-fast">
					<a href="/" class="not-a-like">
						<span class="icone-logo txtmiddle"></span>
						<span class="txtsub">Say it with Kittens</span>
					</a>
				</div>

				<div class="menu-barre-search menu-sharing-photo inbl right proxima h6-like pam transition-fast">
					<a class="not-a-like" href="/upload/">
						<span class="small-hidden tiny-hidden">Share your cat photo </span>
						<i class="fa fa-camera-retro pls"></i>
					</a>
				</div>

				<div class="menu-barre-search inbl right proxima-bold h4-like pam transition-fast dnone">
					<a class="not-a-like" id="search-button">Search <i class="fa fa-search pls"></i></a>
				</div>
			</div>
		</div>

